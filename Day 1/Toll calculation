#include<stdio.h>
#include<string.h>
int main()
{
   int vehicalcount;
   char vehicaltype[10];
   printf("Enter the vehicalcount \n:");
   scanf("%d",&vehicalcount);
   float collection =0;
   for( int i;i<vehicalcount;i++)
   sssss
     printf("Enter the  vehical type\n:");
     scanf("%d",&vehicaltype);sssss
     if(strcpy(vehicaltype,"Car")==0)
     {
      collection+=50;

     }
     else if(strcpy(vehicaltype,"Truck")==0)
     {
      collection+=100;
     }
     else if(strcpy(vehicaltype,"bike")==0)
     {
      collection+=20;
     }
   
   printf("Total Toll collection:%0.2f\n",collection);
   return 0;
}
